{
  "name": "angular2-starter",
  "version": "0.1.0",
  "description": "Angular2 // ES6+ // JSPM Starter",
  "main": "index.js",
  "scripts": {
    "postinstall": "jspm install",
    "start": "browser-sync start --server --files app/**/*",
    "start-hot": "jspm bundle index.js -id; browser-sync start --server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ryan Pierce",
  "license": "ISC",
  "jspm": {
    "dependencies": {
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.0.0-rc.1",
      "babel-plugin-transform-decorators-legacy": "npm:babel-plugin-transform-decorators-legacy@^1.3.4",
      "json": "github:systemjs/plugin-json@^0.1.2",
      "lodash": "npm:lodash@^4.11.1",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@^2.0.0-rc.1",
      "@angular/compiler": "npm:@angular/compiler@^2.0.0-rc.1",
      "@angular/core": "npm:@angular/core@^2.0.0-rc.1",
      "@angular/platform-browser": "npm:@angular/platform-browser@^2.0.0-rc.1",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "rxjs": "npm:rxjs@5.0.0-beta.6",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "zone.js": "npm:zone.js@0.6.12"
    },
    "overrides": {
      "npm:@angular/common@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.13.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.3": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      }
    }
  },
  "devDependencies": {
    "browser-sync": "^2.12.3",
    "jspm": "^0.17.0-beta.22"
  },
  "dependencies": {
    "jspm": "^0.16.34"
  }
}
